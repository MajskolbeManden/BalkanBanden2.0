<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:something="clr-namespace:chatshit.ViewModels"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="chatshit.chatpage">
    <ContentPage.BindingContext>
        <something:ItemViewModel/>
    </ContentPage.BindingContext>
    <ContentPage.Content>
        
        <StackLayout>
            <Label BackgroundColor="Blue" Text="{Binding LineOne, Mode=TwoWay}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>